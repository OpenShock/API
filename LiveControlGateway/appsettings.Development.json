{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft.Hosting.Lifetime": "Verbose",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Verbose",
        "Serilog.AspNetCore.RequestLoggingMiddleware": "Information",
        "Microsoft.AspNetCore.Mvc.Infrastructure.ControllerActionInvoker": "Warning",
        "OpenShock": "Verbose"
      }
    }
  }
}
